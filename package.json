{
  "name": "atlas-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:data:tier-a": "python3 scripts/build_tier_a_data.py",
    "build:data:tier-b": "python3 scripts/build_tier_b_data.py",
    "build:data:tier-c": "python3 scripts/build_tier_c_data.py",
    "build:data": "npm run build:data:tier-a && npm run build:data:tier-b && npm run build:data:tier-c",
    "freeze:tier-a": "npm run build:data:tier-a && python3 scripts/freeze_tier_a_baseline.py",
    "freeze:tier-b": "npm run build:data && python3 scripts/freeze_tier_b_baseline.py",
    "freeze:tier-c": "npm run build:data && python3 scripts/freeze_tier_c_baseline.py",
    "dev": "npm run build:data && vite",
    "build": "npm run build:data && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.4.19"
  }
}
